<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<% flash.each do |key, message| %>
    <div class="alert bg-light text-danger alert-<%= key %>">
      <%= message %>
    </div>
  <% end %>
<%= form_with(url: sessions_path, method: "post") do |f| %>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <div>
      <%= f.submit "Log in" %>
    </div>
  <% end %>